{
    "name": "fast-api/permission",
    "description": "common rbac permission",
    "keywords": [
        "rbac",
        "laravel",
        "permission"
    ],
    "homepage": "https://github.com/fast-php/fast-api-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "chengyu liu",
            "email": "liuchnegyu@live.cn"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "darkaonline/l5-swagger": "^8.0",
        "laravel/framework": ">=7.24",
        "tymon/jwt-auth": "1.0.0",
        "prettus/l5-repository": "2.6.x",
        "dingo/api": "3.0.0",
        "lcobucci/jwt": "3.3.0",
        "mews/captcha": "^3.2"
    },
    "require-dev": {
        "php": ">=7.2.5",
        "darkaonline/l5-swagger": "^8.0",
        "laravel/framework": ">=7.24",
        "tymon/jwt-auth": "1.0.0",
        "prettus/l5-repository": "2.6.x",
        "dingo/api": "3.0.0",
        "lcobucci/jwt": "3.3.0",
        "mews/captcha": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Fast\\Api\\Permission\\": "src"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Fast\\Api\\Permission\\PermissionServiceProvider"
            ]
        }
    }
}
